#include <stdio.h>

void find_repeated_elements(int *arr, int n) {
  int i, j;
  int count[100] = {0};

  for (i = 0; i < n; i++) {
    count[arr[i]]++;
  }

  for (i = 0; i < n; i++) {
    if (count[arr[i]] == 2) {
      printf("%d ", arr[i]);
    }
  }
}

int main() {
	printf(" mahesh reddy \n 192210708 \n");
  int arr[] = {1, 2, 3, 2, 4, 1, 5, 6};
  int n = sizeof(arr) / sizeof(arr[0]);

  find_repeated_elements(arr, n);

  return 0;
}
