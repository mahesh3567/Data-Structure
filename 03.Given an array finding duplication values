#include <stdio.h>

void find_duplicates(int *arr, int n) {
  int i, j;

  for (i = 0; i < n; i++) {
    for (j = i + 1; j < n; j++) {
      if (arr[i] == arr[j]) {
        printf("%d ", arr[i]);
      }
    }
  }
}

int main() {
	printf(" mahesh reddy \n 192210708 \n");
  int arr[] = {1, 2, 3, 2, 5, 7, 8, 8};
  int n = sizeof(arr) / sizeof(arr[0]);

  find_duplicates(arr, n);

  return 0;
}
