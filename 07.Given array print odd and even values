#include <stdio.h>

void print_odd_even_values(int *arr, int n) {
  int i;

  printf("Odd numbers: ");
  for (i = 0; i < n; i++) {
    if (arr[i] % 2 != 0) {
      printf("%d ", arr[i]);
    }
  }

  printf("\nEven numbers: ");
  for (i = 0; i < n; i++) {
    if (arr[i] % 2 == 0) {
      printf("%d ", arr[i]);
    }
  }

  printf("\n");
}

int main() {
	printf(" mahesh reddy \n 192210708 \n");
  int arr[] = {1, 2, 3, 4, 5, 6, 7, 8};
  int n = sizeof(arr) / sizeof(arr[0]);

  print_odd_even_values(arr, n);

  return 0;
}
