#include <stdio.h>

int reverse(int n) {
  int reversed = 0;
  int digit;

  while (n != 0) {
    digit = n % 10;
    reversed = reversed * 10 + digit;
    n /= 10;
  }

  return reversed;
}

int main() {
	printf(" mahesh reddy \n 192210708 \n");
  int x = 12345;
  int reversed = reverse(x);

  printf("Original integer: %d\n", x);
  printf("Reversed integer: %d\n", reversed);

  return 0;
}
