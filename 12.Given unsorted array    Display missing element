#include <stdio.h>

int main() {
	printf(" mahesh reddy \n 192210708 \n");
  int arr[] = {1, 2, 3, 4, 6, 7, 8};
  int n = sizeof(arr) / sizeof(arr[0]);

  int i, j;

  int sum = 0;
  for (i = 0; i < n; i++) {
    sum += arr[i];
  }

  int expected_sum = (n + 1) * (n + 2) / 2;

  int missing_element = expected_sum - sum;

  printf("The missing element is: %d\n", missing_element);

  return 0;
}
