#include <stdio.h>

void concatenate_arrays(int *arr1, int *arr2, int n1, int n2, int *arr3) {
  int i, j;

  for (i = 0; i < n1; i++) {
    arr3[i] = arr1[i];
  }

  for (j = 0; j < n2; j++) {
    arr3[i + j] = arr2[j];
  }
}

int main() {
	printf(" mahesh reddy \n 192210708 \n");
  int arr1[] = {1, 2, 3, 4, 5};
  int arr2[] = {6, 7, 8, 9, 10};
  int n1 = sizeof(arr1) / sizeof(arr1[0]);
  int n2 = sizeof(arr2) / sizeof(arr2[0]);
  int arr3[n1 + n2];

  concatenate_arrays(arr1, arr2, n1, n2, arr3);
  int i;
  for (i = 0; i < n1 + n2; i++) {
    printf("%d ", arr3[i]);
  }

  printf("\n");

  return 0;
}
